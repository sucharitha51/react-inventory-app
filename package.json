{
  "homepage": "InventoryList",
  "name": "inventory-list",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "css-loader": "^3.5.3",
    "identity-obj-proxy": "^3.0.0",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-json-to-csv": "^1.0.4",
    "react-responsive-modal": "^5.0.2",
    "react-scripts": "3.4.1",
    "react-test-renderer": "^16.13.1",
    "style-loader": "^1.2.1",
    "webpack": "^4.42.0"
  },
  "scripts": {
